@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

# Find required dependencies
find_dependency(Threads REQUIRED)

# Include the targets file
include("${CMAKE_CURRENT_LIST_DIR}/ThreadSafeMsgQueueTargets.cmake")

# Set variables for backward compatibility
set(ThreadSafeMsgQueue_FOUND TRUE)
set(ThreadSafeMsgQueue_VERSION "@PROJECT_VERSION@")
set(ThreadSafeMsgQueue_VERSION_MAJOR "@PROJECT_VERSION_MAJOR@")
set(ThreadSafeMsgQueue_VERSION_MINOR "@PROJECT_VERSION_MINOR@")
set(ThreadSafeMsgQueue_VERSION_PATCH "@PROJECT_VERSION_PATCH@")

# Provide imported target alias
if(NOT TARGET ThreadSafeMsgQueue::ThreadSafeMsgQueue)
    add_library(ThreadSafeMsgQueue::ThreadSafeMsgQueue ALIAS ThreadSafeMsgQueue)
endif()

check_required_components(ThreadSafeMsgQueue)
